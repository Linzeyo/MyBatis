<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
    <select id="list" resultType="com.linze.pojo.User">
        select * from users
    </select>
    <select id="getUserById" resultType="com.linze.pojo.User">
        select * from users where uid = #{id}
    </select>
</mapper>
